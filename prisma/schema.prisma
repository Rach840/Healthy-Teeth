generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Category{
  id Int @id @default(autoincrement())
name String @unique
image String 
services Services[]   
}
model Services {
 id Int @id @default(autoincrement())
name String
image String 
price Int
categoryName String
category Category @relation(fields: [categoryName], references: [name]) 
description Json?
}
